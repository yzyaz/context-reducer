{
	"name": "context-reducer",
	"version": "1.0.1",
	"description": "使用useContext和useReducer的一个轻量状态管理库",
	"source": "src/index.tsx",
	"main": "dist/context-reducer.js",
	"module": "dist/context-reducer.mjs",
	"unpkg": "dist/context-reducer.umd.js",
	"types": "dist/context-reducer.d.ts",
	"amdName": "ContextReducer",
	"sideEffects": false,
	"scripts": {
		"example": "parcel example/index.html",
		"build": "microbundle --jsx React.createElement --external react --globals react=React --strict --no-compress -o dist"
	},
	"keywords": [
		"context",
		"useContext",
		"useReducer",
		"redux"
	],
	"author": "yzyaz",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.5",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.12",
		"husky": "^7.0.4",
		"immer": "^9.0.12",
		"lint-staged": "^12.3.4",
		"microbundle": "^0.14.2",
		"parcel": "^2.3.2",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"ts-node": "^10.6.0",
		"typescript": "^4.6.2",
		"use-immer": "^0.6.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yzyaz/context-reducer",
		"directory": ""
	},
	"bugs": {
		"url": "https://github.com/facebook/create-react-app/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": [
			"prettier --write"
		]
	}
}
